<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="pe.edu.pamer.aula.pamer.activities.ClavesActivity"
    android:padding="10dp"
    android:id="@+id/indiv_clave"
    android:background="@drawable/layout_border">

    <android.support.constraint.Guideline
        android:id="@+id/claves_indiv_vert10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <android.support.constraint.Guideline
        android:id="@+id/claves_indiv_vert20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.2" />

    <android.support.constraint.Guideline
        android:id="@+id/claves_indiv_vert65"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.65" />

    <android.support.constraint.Guideline
        android:id="@+id/claves_indiv_vert74"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.743" />

    <android.support.constraint.Guideline
        android:id="@+id/claves_indiv_vert84"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.842" />

    <TextView
        android:id="@+id/textViewIndivClavePar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="par"
        android:textSize="10sp"
        android:textColor="@color/black"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/claves_indiv_vert10"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewIndivClaveNro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nro"
        android:textSize="10sp"
        android:textColor="@color/black"
        android:layout_margin="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/claves_indiv_vert20"
        app:layout_constraintStart_toStartOf="@id/claves_indiv_vert10"
        app:layout_constraintHorizontal_bias="0.40"/>

    <TextView
        android:id="@+id/textViewIndivClaveCur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="curso"
        android:textSize="10sp"
        android:textColor="@color/black"
        android:layout_margin="0dp"
        app:layout_constraintEnd_toStartOf="@id/claves_indiv_vert65"
        app:layout_constraintStart_toStartOf="@id/claves_indiv_vert20"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

    <TextView
        android:id="@+id/textViewIndivClaveClM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="clM"
        android:textSize="10sp"
        android:textColor="@color/black"
        android:layout_margin="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/claves_indiv_vert65"
        app:layout_constraintEnd_toStartOf="@id/claves_indiv_vert74" />

    <TextView
        android:id="@+id/textViewIndivClaveClC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="clC"
        android:textSize="10sp"
        android:textColor="@color/black"
        android:layout_margin="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/claves_indiv_vert74"
        app:layout_constraintEnd_toStartOf="@id/claves_indiv_vert84"
        app:layout_constraintHorizontal_bias="0.70"/>

    <TextView
        android:id="@+id/textViewIndivClavePun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="pun"
        android:textSize="10sp"
        android:textColor="@color/black"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="0dp"
        android:layout_marginStart="25dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/claves_indiv_vert84" />


</android.support.constraint.ConstraintLayout>